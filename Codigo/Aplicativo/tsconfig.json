{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "lib": ["ES2015", "DOM"],
    "target": "ES2015",
    "jsx": "react",
    "strict": true,
    "typeRoots": ["./types"],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "metro.config.js"  ]
}
